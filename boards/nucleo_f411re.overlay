/ {
    aliases{
        myadc = &adc1;
        myadcchannel = &channel0;
    };
};
&adc1 {
    #address-cells = <1>;
    #size-cells = <0>;
    st,adc-prescaler = <6>;
    status = "okay";
    channel0: channel@0 {
        reg = <0>;
        zephyr,gain = "ADC_GAIN_1";
        zephyr,reference = "ADC_REF_INTERNAL";
        zephyr,vref-mv = <3300>;
        zephyr,acquisition-time= <ADC_ACQ_TIME_DEFAULT>;
        zephyr,resolution = <12>;
    };
};

